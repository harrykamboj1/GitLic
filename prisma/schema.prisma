generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id        String   @id @default(uuid())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    imageUrl  String?
    firstName String?
    lastName  String?

    emailAddresses String @unique

    credits Int @default(150)
}
